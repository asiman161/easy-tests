    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/router": "3.4.7",
    "@angular/upgrade": "2.4.7",
    "@types/lodash": "^4.14.52",
    "angular2-token": "^0.2.0-beta.8 ",
    "angularjs-toaster": "^2.1.0",
    "core-js": "^2.4.1",
    "flexboxgrid": "^6.3.1",
